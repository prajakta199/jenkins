pipeline {
  agent any
tools {
    nodejs "node"
}

  stages {
    stage('Setup and Run Tests on BrowserStack') {
      steps {
        browserstack(credentialsId: 'ce17c56d-ea58-4b36-84cf-8532bbbc6e03') {
          sh 'npm install'
          sh 'npx playwright install --with-deps'
          sh 'npm run sample-test'

        }
      }
    }
  }
}
